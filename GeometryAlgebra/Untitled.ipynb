    "import sympy as sm\n",
    "import sympy.physics.vector\n",
    "B = sm.physics.vector.ReferenceFrame(\"B\")\n",
    "u = B.x + B.y\n",
    "v = B.x + 2*B.y\n",
    "vi = (1/(v&v)*v)\n",
    "(vi|u).to_matrix(B)"
   ]
